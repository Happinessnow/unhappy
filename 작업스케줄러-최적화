rem Group Policy Update Force - Web + Fri(12:00), Every OnLogon, Every OnStart
schtasks /create /tn"GP_Update" /tr"%windir%\system32\gpupdate.exe /force" /d wed,fri /st 12:00
schtasks /create /tn"GP_Update" /tr"%windir%\system32\gpupdate.exe /force" /sc onlogon
schtasks /create /tn"GP_Update" /tr"%windir%\system32\gpupdate.exe /force" /sc onstart
